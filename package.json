{
  "name": "codestar-events-website",
  "version": "1.0.0",
  "description": "Elm rebuild of codestar.nl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "elm-format app/src --yes",
    "format-validate": "elm-format app/src --validate",
    "build": "./node_modules/.bin/webpack",
    "start": "npm run format && ./node_modules/.bin/concurrently \"node scripts/server.js\" \"./node_modules/.bin/webpack-dev-server\"",
    "postinstall": "elm package install --yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-star/codestar-events-website.git"
  },
  "private": true,
  "author": "Codestar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-star/codestar-events-website/issues"
  },
  "homepage": "https://github.com/code-star/codestar-events-website#readme",
  "dependencies": {
    "del": "3.0.0",
    "merge-stream": "1.0.1"
  },
  "peerDependencies": {
    "http-server": "^0.10.0",
    "elm": "^0.18.0",
    "elm-format": "^0.7.0-exp"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "cors-anywhere": "0.4.0",
    "css-loader": "^0.28.7",
    "elm-assets-loader": "^0.3.0",
    "elm-webpack-loader": "^4.4.0",
    "file-loader": "^1.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
