{
  "name": "elm-codestar-website",
  "version": "1.0.0",
  "description": "Elm rebuild of codestar.nl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "elm-format app/src --yes",
    "format-validate": "elm-format app/src --validate",
    "build": "npm run format && elm make app/src/Main.elm --output=docs/main.js",
    "deploy": "elm make app/src/Main.elm --output=docs/main.js",
    "watch": "npm-watch",
    "watch-alt": "node watch.js",
    "start": "npm run format && elm-reactor",
    "postinstall": "elm package install --yes"
  },
  "watch": {
    "develop": "app/**/*.elm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-star/elm-codestar-website.git"
  },
  "private": true,
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-star/elm-codestar-website/issues"
  },
  "homepage": "https://github.com/code-star/elm-codestar-website#readme",
  "dependencies": {
    "npm-watch": "^0.2.0"
  },
  "peerDependencies": {
    "http-server": "^0.10.0",
    "elm": "^0.18.0",
    "elm-format": "^0.7.0-exp"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
